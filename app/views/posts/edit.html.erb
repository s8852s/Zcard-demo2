<h1>編輯文章</h1>

<%= link_to "返回上一頁", :back %>

 <!-- board_path(@board)。 -->

<%= form_with(model: @post, url: post_path(@post), method: "put", local: true) do |form| %>

<div class="fields">
    <%= form.label :title, "標題" %>
    <%= form.text_field :title %>
</div>

<div class="fields">
    <%= form.label :content, "內容" %>
    <%= form.text_area :content %>
</div>

<%= form.submit "發表文章" %>

<% end %>
